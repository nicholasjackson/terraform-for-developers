{
  "//": {
    "metadata": {
      "backend": "local",
      "stackName": "static_app",
      "version": "0.12.3"
    },
    "outputs": {
      "static_app": {
        "uri": "uri"
      }
    }
  },
  "output": {
    "uri": {
      "value": "${digitalocean_app.static_site.live_url}"
    }
  },
  "resource": {
    "digitalocean_app": {
      "static_site": {
        "//": {
          "metadata": {
            "path": "static_app/static_site",
            "uniqueId": "static_site"
          }
        },
        "spec": {
          "name": "${var.name}",
          "region": "${var.region}",
          "static_site": [
            {
              "github": {
                "branch": "main",
                "deploy_on_push": true,
                "repo": "nicholasjackson/mame-wasm"
              },
              "name": "${var.name}",
              "source_dir": "/src"
            }
          ]
        }
      }
    }
  },
  "terraform": {
    "required_providers": {
      "digitalocean": {
        "source": "digitalocean/digitalocean",
        "version": "2.22.3"
      }
    }
  },
  "variable": {
    "name": [
      {
        "default": "module",
        "description": "Name of the DigitalOcean static site"
      }
    ],
    "region": [
      {
        "default": "ams",
        "description": "Region to deploy the app to"
      }
    ]
  }
}